type Query {
    jobByTitle(title: String!): [Job]
    getAllJobs: [Job]
}

type Job {
    id: ID!
    jobId: String!
    title: String!
    companyName: String
    location: String
    via: String
    description: String
    extensions: String
    remoteOption: Boolean
    minSalary: Float
    maxSalary: Float
    avgSalary: Float
    eduDegree: [String]
    skills: [String]
    postedAt: String
    scheduleType: String
}

schema {
    query: Query
}